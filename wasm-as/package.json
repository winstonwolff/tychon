{
  "name": "tychon-interpreter",
  "description": "Interprets Tychon low-level S-expressions",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "test": "npx asp --verbose",
    "test:watch": "npx asp --watch",
    "asbuild:debug": "asc assembly/*.ts --target debug",
    "asbuild:release": "asc assembly/*.ts --target release",
    "asbuild": "echo '\n\n' && npm run asbuild:debug && npm run asbuild:release",
    "watch": "ls package.json assembly/*.ts | entr npm run asbuild",
    "start": "npx serve .",
    "clean": "rm -rf build/"
  },
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "dependencies": {
    "assemblyscript-json": "^1.1.0"
  },
  "devDependencies": {
    "@as-pect/cli": "^8.1.0",
    "aspect": "^0.0.2",
    "assemblyscript": "^0.27.10"
  }
}
